{
	"name": "Dark-Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#0f0f13",
		"editor.foreground": "#dfdfdf",
		"activityBarBadge.background": "#006ab1",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#131313",
		"list.activeSelectionForeground": "#b6f30d",
		"list.activeSelectionBackground": "#5d70a7",
		"tab.inactiveForeground": "#cec9e9",
		"tab.activeBackground": "#262A33",
		"tab.inactiveBackground": "#23262E",
		"tab.activeForeground": "#00e8c6"
	},
	"tokenColors": [
		{
			"name": "Comment color",
			"scope": ["comment", "markup.quote.markdown", "meta.diff", "meta.diff.header"],
			"settings": {
				"foreground": "#8d8689"
			}
		},
		{
			//variables en php y javascript
			//funciones javascript
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#0b51a1"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#2980b9",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Text Color",
			"scope": [
				"meta.template.expression.js",
				"constant.name.attribute.tag.jade",
				"punctuation.definition.metadata.markdown",
				"punctuation.definition.string.end.markdown",
				"punctuation.definition.string.begin.markdown"
			],
			"settings": {
				"foreground": "#D5CED9"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"variable",
				"support.variable",
				"entity.name.tag.yaml",
				"constant.character.entity.html",
				"source.css entity.name.tag.reference",
				"beginning.punctuation.definition.list.markdown",
				"source.css entity.other.attribute-name.parent-selector",
				"meta.structure.dictionary.json support.type.property-name"
			],
			"settings": {
				"foreground": "#00e8c6"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"markup.bold",
				"constant.numeric",
				"meta.group.regexp",
				"constant.other.php",
				"support.constant.ext.php",
				"constant.other.class.php",
				"support.constant.core.php",
				"fenced_code.block.language",
				"constant.other.caps.python",
				"entity.other.attribute-name",
				"support.type.exception.python",
				"source.css keyword.other.unit",
				"variable.other.object.property.js.jsx",
				"variable.other.object.js"
			],
			"settings": {
				//variables en funcion java
				"foreground": "#ff9e02"
			}
		},
		{
			//etiquetas html
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
			"settings": {
				"foreground": "#ff1702"
			}
		},
		{
			"name": "Yellow",
			"scope": [
				"markup.list",
				"text.xml string",
				"entity.name.type",
				"support.function",
				"entity.other.attribute-name",
				"meta.at-rule.extend",
				"entity.name.function",
				"entity.other.inherited-class",
				"entity.other.keyframe-offset.css",
				"text.html.markdown string.quoted",
				"meta.function-call.generic.python",
				"meta.at-rule.extend support.constant",
				"entity.other.attribute-name.class.jade",
				"source.css entity.other.attribute-name",
				"text.xml punctuation.definition.string"
			],
			"settings": {
				//clases en html
				"foreground": "#e5f132"
			}
		},
		{
			"name": "Pink",
			"scope": ["markup.heading", "variable.language.this.js", "variable.language.special.self.python"],
			"settings": {
				"foreground": "#ff00aa",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Hot Pink",
			"scope": [
				"punctuation.definition.interpolation",
				"punctuation.section.embedded.end.php",
				"punctuation.section.embedded.end.ruby",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.begin.ruby",
				"punctuation.definition.template-expression",
				"entity.name.tag"
			],
			"settings": {
				//etiquetas html
				"foreground": "#f34333",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Purple",
			"scope": ["storage", "keyword", "meta.link", "meta.image", "markup.italic", "source.js support.type"],
			"settings": {
				//reservadas jsvascript var let document
				//php public reservadas sql
				"foreground": "#c74ded",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Blue",
			"scope": ["string.regexp", "markup.changed"],
			"settings": {
				"foreground": "#7cb7ff"
			}
		},
		{
			"name": "Red",
			"scope": [
				"constant",
				"support.class",
				"keyword.operator",
				"support.constant",
				"text.html.markdown string",
				"source.css support.function",
				"source.php support.function",
				"support.function.magic.python",
				"entity.other.attribute-name.id",
				"markup.deleted"
			],
			"settings": {
				//phpreservadas flechas sql nombre de campos
				"foreground": "#f85f44"
			}
		},
		{
			"name": "Green",
			"scope": [
				"string",
				"text.html.php string",
				"markup.inline.raw",
				"markup.inserted",
				"punctuation.definition.string",
				"punctuation.definition.markdown",
				"text.html meta.embedded source.js string",
				"text.html.php punctuation.definition.string",
				"text.html meta.embedded source.js punctuation.definition.string",
				"text.html punctuation.definition.string",
				"text.html string"
			],
			"settings": {
				//nombre de tablas php atributos de clase html
				"foreground": "#72ff2c"
			}
		},
		{
			"name": "Font Underline",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"fontStyle": "underline"
			}
		}
	]
}
